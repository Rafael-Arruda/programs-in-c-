#include <stdio.h>

int main(void) {
  int n;
  int f1,f2;
  int mdc;
  printf("Digite a quantidade de casos teste: ");
  scanf("%i",&n);
  int i,j;
  for(i=0;i<n;i++){
    printf("Digite os valores de n1 e n2: ");
    scanf("%i %i",&f1,&f2);
    for(j=1;j<=f1;j++){
      if(f1%j ==0 && f2%j ==0){
        mdc = j;
      }
    }
    printf("O mdc vale: %i\n",mdc);
  }
  return 0;
}
